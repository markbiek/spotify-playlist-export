{
	"name": "mark/spotify-playlist-export",
	"description": "A PHP application for Spotify playlist export",
	"type": "project",
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		},
		"files": [
			"src/Helpers/AuthHelpers.php",
			"src/Helpers/SecurityHelper.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"require": {
		"php": ">=8.2",
		"jwilsson/spotify-web-api-php": "^6.2",
		"twig/twig": "^3.20"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0"
	},
	"scripts": {
		"test": "phpunit"
	}
}